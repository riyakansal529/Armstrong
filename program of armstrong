#include <stdio.h>
#include <conio.h>
int main()
{
    int n,r,c,arm=0;
    printf("Enter any Number:");
    scanf("%d", &n);
    c=n;
    while(n>0)
    {
        r=n%10;
        arm=(r*r*r)+arm;
        n=n/10;
    }
    if(c==arm)
    printf("it is a armstrong");
     else
     printf("it is not a armstrong");
     getch();
}
